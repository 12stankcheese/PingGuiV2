-- Scripts:

local function XLHEOK_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local db = true
	local player = game.Players.LocalPlayer
	local delay = 3
	local button = script.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		if player.Name == "IndianRedPing" or player.Name == "glizzylou" or player.Name == "PeptoBismoII" or player.Name == "YoLocalTay" then
			button.Parent.Parent.TextButton.Visible = true
			script.Parent:Destroy()
		else
			button.Text = "LOL NIGGER NOT WHITELISTED"
			wait(2)
			script.Parent.Parent.Frame.Visible = true
		end
	end)
end
coroutine.wrap(XLHEOK_fake_script)()
local function YERVHE_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local db = true
	local player = game.Players.LocalPlayer
	local delay = 3
	
	
	script.Parent.MouseButton1Click:Connect(function()
		if db == true and game.Workspace.Dealers:FindFirstChild("Dealer2") then
			db = false
			player.Character.HumanoidRootPart.CFrame = game.Workspace.Dealers.Dealer2.Head.CFrame * CFrame.new(0, 3, 0)
			for i = delay, 1, -1 do
				script.Parent.Text = i
				wait(1)
			end
			script.Parent.Text = "Dealer2"
			db = true
		elseif db == true and not game.Workspace.Dealers:FindFirstChild("Dealer2") then
			script.Parent.Text = "Dealer Not Found"
			db = false
			wait(1)
			script.Parent.Text = "Dealer2"
			db = true
		end
	end)
end
coroutine.wrap(YERVHE_fake_script)()
