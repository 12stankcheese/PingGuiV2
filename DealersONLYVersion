local function KKVUT_fake_script() -- TextButton_10.LocalScript 
	local script = Instance.new('LocalScript', TextButton_10)

	local db = true
	local player = game.Players.LocalPlayer
	local delay = 3
	local button = script.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		if db == true then
			button.Parent.Frame2.Visible = true
			db = false
		elseif db == false then
			button.Parent.Frame2.Visible = false
			db = true
		end
	end)
end
coroutine.wrap(KKVUT_fake_script)()
